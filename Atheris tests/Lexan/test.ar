import stdlib

// if, else if, else

var x = 10

if x == 10 {
  print(1)
}
else if x == 11 {
  print(2)
}
else if x == 35 {
  print("lorem ipsum")
}
else {
  print(33)
}

// switch case

switch x {
  // veƒç pogojev skupaj
case 1, 2, 3:
  print("ena, dva ali tri")
  break
case 10:
  print("x = 10")
case 11:
  print("x = 11")
  break
default:
  print("Ce ni breaka se izvedem tudi jaz")
}


// while
  
x = 0
while x < 100 {
  if x % 2 == 0 {
    x = x + 5
    print(x)
    continue
  }
  else if x == 50 {
    break
  }
  x = x + 1
}

// for
for i in [1, 2, 5, 10, 100, 333] {
  print(i)
}

